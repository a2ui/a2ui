{
  "name": "a2ui",
  "version": "1.0.5",
  "license": "MIT",
  "url" : "http://a2ui.codeweaver.pl/",
  "scripts": {
    "prestart": "tslint -c tslint.json \"src/**/*.ts\"",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build:release": "npm run build:clean && npm run build:compile && npm run build:dt && npm run build:assets && npm run build:publish",
    "build:clean": "rimraf dist",
    "build:compile": "node ./node_modules/@angular/compiler-cli/src/main.js -p tsconfig.release.json",
    "build:dt": "node ./node_modules/@angular/compiler-cli/src/main.js -p tsconfig.release.dt.json",
    "build:assets": "node scripts/build.js",
    "build:publish": "cd dist && npm publish",
    "rimraf ": "rimraf"
  },
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "0.6.4",
    "@angular/platform-server": "^2.0.2",
    "@types/bootstrap": "*",
    "@types/core-js": "*",
    "@types/jasmine": "*",
    "@types/jquery": "*",
    "@types/node": "*",
    "bootstrap": "^3.3.6",
    "concurrently": "^2.0.0",
    "copy": "^0.3.0",
    "jquery": "^2.2.4",
    "lite-server": "^2.2.0",
    "material-design-iconic-font": "^2.2.0",
    "rimraf": "^2.5.4",
    "tslint": "^3.11.0",
    "typescript": "2.0.3"
  }
}
